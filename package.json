{
    "name": "vscode-react-refactor",
    "displayName": "VSCode React Refactor",
    "description": "Recompose your overgrown JSX without worrying about the given data.",
    "publisher": "planbcoding",
    "repository": "https://github.com/planbcoding/vscode-react-refactor.git",
    "icon": "assets/images/icon.png",
    "author": "planbcoding",
    "keywords": [
        "react",
        "refactor",
        "tools"
    ],
    "license": "ISC",
    "version": "1.0.5",
    "engines": {
        "vscode": "^1.1.21"
    },
    "main": "./out/src/extension",
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./"
    },
    "activationEvents": [
        "workspaceContains:package.json"
    ],
    "contributes": {
        "commands": [
            {
                "command": "extension.react-refactor.extractToClass",
                "title": "Extract JSX to Class Component"
            },
            {
                "command": "extension.react-refactor.extractToFunction",
                "title": "Extract JSX to Functional Component"
            }
        ],
        "menus": {
            "explorer/context": []
        },
        "configuration": [
            {
                "title": "VSCode React Refactor",
                "properties": {
                    "vscodeReactRefactor.functionType": {
                        "type": "string",
                        "enum": [
                            "function",
                            "arrowFunction"
                        ],
                        "default": "function",
                        "description": "The created React Functional Component will be"
                    },
                    "vscodeReactRefactor.babel.customPlugins": {
                        "type": "string",
                        "default": "",
                        "description": "Additional plugin names for @babel/parser to use (eg: flow, flowComments)"
                    }
                }
            }
        ]
    },
    "dependencies": {
        "@babel/core": "^7.12.10",
        "@babel/parser": "^7.12.11",
        "@babel/template": "^7.12.7",
        "@babel/traverse": "^7.12.12",
        "@babel/types": "^7.12.12",
        "lines-and-columns": "^1.1.6",
        "lodash.pickby": "^4.6.0"
    },
    "devDependencies": {
        "@types/lodash.pickby": "^4.6.6",
        "@types/node": "^14.14.20",
        "@types/vscode": "^1.52.0",
        "typescript": "^4.1.3"
    }
}
